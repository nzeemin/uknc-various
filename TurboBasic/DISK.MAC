		CMP	@R2,LENW(R1)
		BHI	MMER
		MOV	(R2)+,R5
		MOV	R5,LENR(R1)

		MOV	#774/2,R0
		CMP	R0,R5
		BLO	7$
		MOV	R5,R0
		CLR	R5
		BR	8$
	7$:	SUB	R0,R5

