		.WORD	FILTYP
		.BYTE	3,4
		BR	3$
		BR	1$

	3$:	COMB	@#ASCFLG
		CLR	@#BLKCNT
		MOV	#BUFFIL,@#BUFCNT

	1$:	MOV	(SP)+,R4
		MOV	(SP)+,R3
		MOV	(SP)+,R2
		CLC
		RETURN

	2$:	CMP	LENW(R1),#200
		BHIS	4$
		CLRB	@#ASCFLG

	4$:	MOV	(SP)+,R4
		MOV	(SP)+,R3
		MOV	(SP)+,R2
		SEC
		RETURN
