	1$:

	5$:	MOVB	(R5)+,(R2)+

	3$:	CMP	R5,#BUFFIL+1000

		BLO	2$
		INC	@#BLKCNT
		MOV	#BUFFIL,R5
		.READW	#PARAM,#3,R5,#400,@#BLKCNT
		BCC	2$
		TSTB	@#ERRBYT
		BNE	MAER
		CLRB	@R5

	2$:
		SOB	R3,4$
	6$:	MOV	R5,@#BUFCNT
	MEND1:	BR	MENDA

	MAER:
		MOVB	#RER.,@R1
		MOVB	#RER.,1(R1)
		BR	MEND

