		MOV	@#BUFCNT,R5

	1$:	CMPB	@R2,#32
		BEQ	2$


	7$:	MOVB	(R2)+,(R5)+
	5$:	CMP	R5,#BUFFIL+1000
		BLO	6$
		BR	3$

	2$:	CLRB	(R5)+
		CMP	R5,#BUFFIL+1000
		BLO	2$
		MOV	#1,R3

	3$:	MOV	#BUFFIL,R5
		.WRITW	#PARAM,#0,R5,#400,@#BLKCNT
		BCS	MAER
		INC	@#BLKCNT

	6$:	SOB	R3,1$
		MOV	R5,@#BUFCNT
		BR	MEND1


FilApd:
		Push	r4		; "File.Dat[-1]=\0" ->
		Mov	r3,r2		; -> "File.Dat[-1]=File.Dat\0"
		Mov	r3,r5
		Mov	#ApdBuf,r4
	61$:	Movb	(r2)+,(r4)+
		Bne	61$
		Dec	r4
	62$:	Movb	(r3)+,(r4)+
		Cmpb	#'[,(r3)
		Bne	62$
		Clrb	(r4)
		Pop	r4
		Mov	#ApdBuf,r3
		Return


ApdCpy:		Push	r1
		Clr	ApdId
	2$:	.ReadW	#Param,#3,#BufFil,#400,@#BlkCnt
		Bcc	13$
		Tst	@#52
		Bne	11$
		
		Inc	BlkCnt			; é°‡†°Æ‚™† ‰†©´† °•ß EOF
		Mov	#BufFil,r0
		Clrb	(r0)
		Br	10$
	
	13$:	Mov	#BufFil,r0
		Mov	#1000,r1
	1$:	Tstb	(r0)+
		Beq	10$
		Sob	r1,1$
		.WritW	#Param,#0,#BufFil,#400,@#BlkCnt
		Bcs	11$
		Inc	BlkCnt
		Br	2$
		
	10$:	Push	r3
		Mov	r0,r2
		Call	FndBuf
		Mov	r0,r1
		Clrb	(r0)
		Add	#22,r1
		Mov	#BufFil,r3
		Cmp	r2,#BufFil+400
		Blo	6$
		Add	#400,r3
	6$:	Mov	r3,BufCnt
	3$:	Movb	(r3)+,(r1)+
		Beq	4$
		Incb	(r0)
		Br	3$
	4$:	Pop	r3
		Pop	r1
		Return
	11$:	
		Trap	19.
		Cmp	(Sp)+,(Sp)+
		Jmp	MeR
	
	
		


